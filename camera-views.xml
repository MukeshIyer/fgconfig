<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<PropertyList>
  <sim>
    <frame-rate-throttle-hz type="double">60</frame-rate-throttle-hz>
    <multithreading-mode>CullDrawThreadPerContext</multithreading-mode>
    <rendering>
      <quality-level>3</quality-level>
      <texture-compression type="string">off</texture-compression>
      <draw-mask>
        <aircraft>0</aircraft>
        <models>1</models>
        <terrain>1</terrain>
      </draw-mask>
      <camera-group>
        <window>
          <name type="string">Front</name>
          <display>0</display>
          <screen>0</screen>
          <fullscreen type="bool">true</fullscreen>
          <x>0</x>
          <y>0</y>
          <width>1920</width>
          <height>1080</height>
        </window>
        <window>
          <name type="string">Left</name>
          <display>0</display>
          <screen>0</screen>
          <fullscreen type="bool">true</fullscreen>
          <x>0</x>
          <y>0</y>
          <width>1920</width>
          <height>1080</height>
        </window>
        <window>
          <name type="string">Right</name>
          <display>0</display>
          <screen>0</screen>
          <fullscreen type="bool">true</fullscreen>
          <x>0</x>
          <y>0</y>
          <width>1920</width>
          <height>1080</height>
        </window>
        <window>
          <name type="string">Cockpit</name>
          <display>0</display>
          <screen>0</screen>
          <fullscreen type="bool">false</fullscreen>
          <x>0</x>
          <y>0</y>
          <width>1920</width>
          <height>1080</height>
        </window>
        <camera>
          <name type="string">FrontView</name>
          <window>
            <name>Front</name>
          </window>
          <view>
            <type>lookfrom</type>
            <heading-offset-deg>0</heading-offset-deg>
            <heading-deg type="double">0.0</heading-deg>
            <roll-deg type="double">0.0</roll-deg>
            <pitch-deg type="double">0.0</pitch-deg>
          </view>
          <!-- <master-perspective> -->
          <!-- <eye-distance type="double">0.7</eye-distance> -->
          <!-- <x-offset type="double">0.0</x-offset> -->
          <!-- <y-offset type="double">+-0.10</y-offset> -->
          <!-- </master-perspective> -->
          <fullscreen type="bool">true</fullscreen>
        </camera>
        <camera>
          <name type="string">RightView</name>
          <window>
            <name>Right</name>
          </window>
          <view>
            <type>lookfrom</type>
            <heading-offset-deg>45</heading-offset-deg>
          </view>
          <right-of-perspective>
            <parent-camera>FrontView</parent-camera>
          </right-of-perspective>
          <fullscreen type="bool">true</fullscreen>
        </camera>
        <camera>
          <name type="string">LeftView</name>
          <window>
            <name>Left</name>
          </window>
          <view>
            <type>lookfrom</type>
            <heading-offset-deg>-45</heading-offset-deg>
          </view>
          <left-of-perspective>
            <parent-camera>FrontView</parent-camera>
          </left-of-perspective>
          <fullscreen type="bool">true</fullscreen>
        </camera>
        <camera>
          <name type="string">CockpitView</name>
          <window>
            <name>Cockpit</name>
          </window>
          <view>
            <heading-deg type="double">0</heading-deg>
            <pitch-deg type="double">-25</pitch-deg>
          </view>
          <fullscreen type="bool">false</fullscreen>
        </camera>
        <gui>
          <window>
            <name type="string">Cockpit</name>
          </window>
        </gui>
      </camera-group>
    </rendering>
  </sim>
</PropertyList>